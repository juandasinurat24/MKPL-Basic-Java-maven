package src.main.java;

public class Counter {
  
  private int count;
  
  public Counter() {
  }
  
  public void reset() {
    count = 0;
  }
  
  public void increment() {
    count++;
  }
  
  public void decrement() {
    count--;
  }
  
  public int getCount() {
    return count;
  }
  
  // Error: method getCount() should return an int value, but it doesn't return anything
}
